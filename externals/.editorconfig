# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*.cs]
end_of_line = crlf
indent_style = space
indent_size = 4
charset = utf-8

[*.{csproj,sln,wixproj}]
end_of_line = crlf

[*.py]
indent_style = space
indent_size = 4
charset = utf-8

[*.clj]
indent_style = space
charset = utf-8

[*.js]
indent_style = space
indent_size = 2
charset = utf-8
